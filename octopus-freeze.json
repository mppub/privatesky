    "create-domain": [
        {
            "name": "apihub-contracts-create-domain",
            "src": "",
            "actions": [
                {
                    "type": "execute",
                    "cmd": "cd modules/apihub-contracts && npm run create-domain"
                }
            ]
        }
    ],
    "prepare-release": [
        {
            "name": "preparing-release",
            "actions": [
                {
                    "type": "remove",
                    "target": "./temp-release/"
                },
                {
                    "type": "remove",
                    "target": "./psknode/bundles/"
                },
                {
                    "type": "execute",
                    "cmd": "node bin/scripts/pskbuild.js --projectMap=./builds/build.json --prod=true"
                }
            ]
        },
        {
            "name": "psk-release",
            "src": "http://github.com/privatesky/psk-release.git",
            "actions": [
                {
                    "type": "smartClone",
                    "target": "temp-release",
                    "options": {
                        "branch": "master"
                    }
                },
                {
                    "type": "remove",
                    "target": "temp-release/psk-release/psknode"
                },
                {
                    "type": "copy",
                    "src": "psknode",
                    "target": "temp-release/psk-release/psknode",
                    "options": {
                        "overwrite": true
                    }
                },
                {
                    "type": "remove",
                    "target": "temp-release/psk-release/psknode/domain.js"
                }
            ]
        }
    ]
}
